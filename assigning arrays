class assign
{
    public static void main(String a[])
    {
        int nums[]={2,8,6,50,80};   //this are the initial values

        nums[0]=28;  //assigned values or changed values
        nums[1]=29;
        nums[2]=259;
        nums[3]=209;

        //for loop to print values
        for(int i=0;i<nums.length;i++)
        {
            System.out.println(nums[i]);
        }
    }
}
