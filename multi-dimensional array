class array
{
    public static void main(String a[])
    {
        int nums[][]=new int[4][5];

        for(int i=0;i<nums.length;i++)
        {
            for(int j=0;j<nums.length;j++)
            {
                System.out.print(nums[i][j] + " ");
            }
                System.out.println();
        }
    }
}
